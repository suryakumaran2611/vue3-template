    "@vue/compiler-sfc": "^3.0.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/test-utils": "^2.0.0-0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "chromedriver": "^84.0.1",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^7.0.0",
    "geckodriver": "^1.20.0",
    "lint-staged": "^9.5.0",
    "node-sass": "^7.0.0",
    "prettier": "^2.2.1",
    "sass-loader": "^8.0.2",
    "typescript": "~3.9.3",
    "vue-cli-plugin-storybook": "^2.1.0",
    "vue-jest": "^5.0.0-0",
    "vue-loader": "^16.3.0"
